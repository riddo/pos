{
  "name": "inertia",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node build/server.js",
    "build": "node ace build && node ace migration:fresh --seed ",
    "dev": "node ace serve --hmr",
    "seed": "node ace migration:fresh --seed",
    "deploy": "npm run build && npm run start --ignore-ts-errors",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.3.96",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@types/vue": "^1.0.31",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-vue": "^1.1.6",
    "tailwindcss": "^3.4.17",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.7.2",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.3"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.4",
    "@adonisjs/bouncer": "^3.1.5",
    "@adonisjs/core": "^6.17.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^2.1.0",
    "@adonisjs/lucid": "^21.5.1",
    "@adonisjs/mail": "^9.2.2",
    "@adonisjs/session": "^7.5.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@inertiajs/vue3": "^2.0.3",
    "@unovis/ts": "^1.5.1",
    "@unovis/vue": "^1.5.1",
    "@vinejs/vine": "^3.0.0",
    "@vue/server-renderer": "^3.5.13",
    "@vueuse/core": "^12.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "edge.js": "^6.2.0",
    "exceljs": "^4.4.0",
    "lucide-vue-next": "^0.474.0",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "pinia": "^3.0.1",
    "radix-vue": "^1.9.13",
    "reflect-metadata": "^0.2.2",
    "reka-ui": "^2.0.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.13",
    "xlsx": "^0.18.5"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "@inertiajs/core": "2.0.0",
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "@inertiajs/core": "2.0.0",
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "@inertiajs/core": "2.0.0",
      "strtok3": "8.0.1"
    }
  },
  "prettier": "@adonisjs/prettier-config"
}
